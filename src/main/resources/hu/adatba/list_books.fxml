<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="hu.adatba.Controller.ListBooksController"
            prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="editprofileBTN" layoutX="201.0" layoutY="150.0" mnemonicParsing="false" onAction="#switchToEditProfile" prefHeight="25.0" prefWidth="150.0" text="Profil adatok" />
    <Button fx:id="addbooksBTN" layoutX="201.0" layoutY="200.0" mnemonicParsing="false" onAction="#switchToAddBooks" prefHeight="25.0" prefWidth="150.0" text="Könyvek felvitele" visible="false" />
    <Button fx:id="logoutBTN" layoutX="201.0" layoutY="230.0" mnemonicParsing="false" onAction="#switchToAddBooks" prefHeight="25.0" prefWidth="150.0" text="Kijelentkezés" />


</AnchorPane>
